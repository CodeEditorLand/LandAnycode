{
	"name": "anycode-go",
	"publisher": "ms-vscode",
	"displayName": "anycode-go",
	"description": "Go for Anycode",
	"license": "MIT",
	"version": "0.0.2",
	"preview": true,
	"repository": {
		"url": "https://github.com/microsoft/vscode-anycode"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"activationEvents": [],
	"browser": "index.js",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"anycode-languages": {
			"grammarPath": "./tree-sitter-go.wasm",
			"languageId": "go",
			"extensions": [
				"go"
			],
			"queryPaths": {
				"comments": "./queries/comments.scm",
				"identifiers": "./queries/identifiers.scm",
				"locals": "./queries/locals.scm",
				"outline": "./queries/outline.scm",
				"references": "./queries/references.scm"
			}
		}
	},
	"scripts": {
		"postinstall": "npx tree-sitter build-wasm node_modules/tree-sitter-go",
		"deploy": "npx vsce publish",
		"test": "node ../anycode/server/src/test-fixture/test.js  --outline ./fixtures/outline.go --highlights ./fixtures/highlights.go"
	},
	"devDependencies": {
		"@playwright/test": "^1.14.1",
		"tree-sitter-cli": "0.20.6",
		"tree-sitter-go": "0.19.1"
	}
}
