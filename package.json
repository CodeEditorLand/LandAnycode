{
	"name": "anycode",
	"displayName": "anycode",
	"publisher": "ms-vscode",
	"description": "",
	"version": "0.0.4",
	"preview": true,
	"repository": {
		"url": "https://github.com/microsoft/vscode-anycode"
	},
	"engines": {
		"vscode": "^1.57.0"
	},
	"extensionKind": [
		"web"
	],
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:csharp",
		"onLanguage:c",
		"onLanguage:cpp",
		"onLanguage:java",
		"onLanguage:php",
		"onLanguage:python",
		"onLanguage:typescript"
	],
	"browser": "./dist/extension.js",
	"main": "./dist/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && npx esbuild src/extension.ts --bundle --outfile=dist/extension.js --platform=node --external:vscode",
		"watch": "npx esbuild src/extension.ts --bundle --outfile=dist/extension.js --platform=node --external:vscode --watch",
		"lint": "eslint src --ext ts",
		"postinstall": "npx tree-sitter build-wasm node_modules/tree-sitter-c && npx tree-sitter build-wasm node_modules/tree-sitter-c-sharp && npx tree-sitter build-wasm node_modules/tree-sitter-cpp && npx tree-sitter build-wasm node_modules/tree-sitter-java && npx tree-sitter build-wasm node_modules/tree-sitter-python && npx tree-sitter build-wasm node_modules/tree-sitter-php npx tree-sitter build-wasm node_modules/tree-sitter-typescript/typescript"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"esbuild": "^0.12.14",
		"eslint": "^7.19.0",
		"tree-sitter-c": "^0.19.0",
		"tree-sitter-c-sharp": "^0.19.0",
		"tree-sitter-cli": "^0.20.0",
		"tree-sitter-cpp": "^0.19.0",
		"tree-sitter-java": "^0.19.1",
		"tree-sitter-php": "^0.19.0",
		"tree-sitter-python": "^0.19.0",
		"tree-sitter-typescript": "^0.19.0",
		"typescript": "^4.1.3"
	}
}
