{
	"name": "anycode-kotlin",
	"displayName": "anycode-kotlin",
	"version": "0.0.1",
	"private": false,
	"description": "Kotlin for Anycode",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"deploy": "npx vsce publish",
		"postinstall": "npx tree-sitter build-wasm node_modules/tree-sitter-kotlin",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"contributes": {
		"anycodeLanguages": {
			"extensions": [
				"kt"
			],
			"grammarPath": "./tree-sitter-kotlin.wasm",
			"languageId": "kotlin",
			"queryPaths": {
				"comments": "./queries/comments.scm",
				"folding": "./queries/folding.scm",
				"identifiers": "./queries/identifiers.scm",
				"locals": "./queries/locals.scm",
				"outline": "./queries/outline.scm",
				"references": "./queries/references.scm"
			}
		},
		"languages": [
			{
				"aliases": [
					"kotlin"
				],
				"extensions": [
					"kt"
				],
				"id": "kotlin"
			}
		]
	},
	"devDependencies": {
		"tree-sitter-cli": "0.22.2",
		"tree-sitter-kotlin": "git+https://github.com/fwcd/tree-sitter-kotlin.git#a4f71eb9b8c9b19ded3e0e9470be4b1b77c2b569",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	},
	"preview": true
}
