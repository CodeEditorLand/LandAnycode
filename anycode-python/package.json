{
	"name": "anycode-python",
	"displayName": "anycode-python",
	"version": "0.0.1",
	"private": false,
	"description": "Python for Anycode",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"deploy": "npx vsce publish",
		"postinstall": "npx tree-sitter build-wasm node_modules/tree-sitter-python",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"contributes": {
		"anycodeLanguages": {
			"extensions": [
				"py",
				"rpy",
				"pyw",
				"cpy",
				"gyp",
				"gypi",
				"pyi",
				"ipy"
			],
			"grammarPath": "./tree-sitter-python.wasm",
			"languageId": "python",
			"queryPaths": {
				"comments": "./queries/comments.scm",
				"identifiers": "./queries/identifiers.scm",
				"locals": "./queries/locals.scm",
				"outline": "./queries/outline.scm",
				"references": "./queries/references.scm"
			},
			"suppressedBy": [
				"ms-python.python"
			]
		}
	},
	"devDependencies": {
		"@playwright/test": "1.42.1",
		"tree-sitter-cli": "0.22.2",
		"tree-sitter-python": "0.21.0",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	},
	"preview": true
}
