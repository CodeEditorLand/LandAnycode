{
	"name": "anycode-python",
	"publisher": "ms-vscode",
	"displayName": "anycode-python",
	"description": "Python for Anycode",
	"license": "MIT",
	"version": "0.0.1",
	"preview": true,
	"repository": {
		"url": "https://github.com/microsoft/vscode-anycode"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"activationEvents": [],
	"browser": "index.js",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"anycode-languages": {
			"grammarPath": "./tree-sitter-python.wasm",
			"languageId": "python",
			"extensions": [
				"py",
				"rpy",
				"pyw",
				"cpy",
				"gyp",
				"gypi",
				"pyi",
				"ipy"
			],
			"queryPaths": {
				"comments": "./queries/comments.scm",
				"identifiers": "./queries/identifiers.scm",
				"locals": "./queries/locals.scm",
				"outline": "./queries/outline.scm",
				"references": "./queries/references.scm"
			}
		}
	},
	"scripts": {
		"postinstall": "npx tree-sitter build-wasm node_modules/tree-sitter-python",
		"deploy": "npx vsce publish",
		"test": "node ../anycode/server/src/test-fixture/test.js  --outline ./fixtures/outline.ts --highlights ./fixtures/highlights.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.14.1",
		"tree-sitter-cli": "0.20.6",
		"tree-sitter-python": "0.19.0"
	}
}
