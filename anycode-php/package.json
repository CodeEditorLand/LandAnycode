{
	"author": {
		"email": "Hello@Playform.Cloud",
		"name": "Playform",
		"url": "https://playform.cloud"
	},
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"contributes": {
		"anycodeLanguages": {
			"extensions": [
				"php",
				"php4",
				"php5",
				"phtml",
				"ctp"
			],
			"grammarPath": "./tree-sitter-php.wasm",
			"languageId": "php",
			"queryPaths": {
				"comments": "./queries/comments.scm",
				"identifiers": "./queries/identifiers.scm",
				"locals": "./queries/locals.scm",
				"outline": "./queries/outline.scm",
				"references": "./queries/references.scm"
			},
			"suppressedBy": [
				"bmewburn.vscode-intelephense-client"
			]
		}
	},
	"description": "PHP for Anycode",
	"devDependencies": {
		"@playwright/test": "1.42.1",
		"tree-sitter-cli": "0.22.2",
		"tree-sitter-php": "0.22.2",
		"typescript-esbuild": ""
	},
	"displayName": "anycode-php",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"license": "SEE LICENSE IN LICENSE",
	"name": "anycode-php",
	"preview": true,
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"scripts": {
		"deploy": "npx vsce publish",
		"postinstall": "npx tree-sitter build-wasm node_modules/tree-sitter-php",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"type": "module",
	"version": "0.0.1"
}
